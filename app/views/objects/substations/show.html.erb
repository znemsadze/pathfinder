<%=view_for @substation, title: @title, icon: '/icons/substation.png', collapsible: true do |f|
  f.text_field 'kmlid', tag: 'code'
  f.text_field 'name'
  f.text_field 'region.name', i18n: 'region', url: region_url(id:@substation.region.id)
  f.text_field 'lat', tag: 'code'
  f.text_field 'lng', tag: 'code'
  f.map_field 'position'
end%>