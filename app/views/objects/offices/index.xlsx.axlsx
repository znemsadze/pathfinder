wb = xlsx_package.workbook
wb.add_worksheet(name: "report") do |sheet|
  sheet.add_row [
    'ID',
    'დასახელება',
    'რეგიონი',
    'მისამართი',
    'შენიშვნა']
  @offices.each do |office|
    sheet.add_row [
      office.id.to_s,     # A
      office.name,        # B
      office.region.to_s, # C
      office.address,     # D
      office.description  # E
    ]
  end
end
